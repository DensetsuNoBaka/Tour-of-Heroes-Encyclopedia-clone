{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "system.webserver": {
      "httpProtocol": {
        "customHeaders": {
          "name": "Access-Control-Allow-Origin",
          "value":  "*"
        }
      }
    }
  }
}
